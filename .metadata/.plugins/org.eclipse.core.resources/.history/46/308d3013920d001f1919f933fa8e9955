/*
 * Crear un programa que muestre los cuadrados y los cubos de los números del 0 al 10.
 */
package EjEstructuraArregloExcepcionesNivelBasico;

public class EjTres {

	    public static void main(String[] args) {
	        System.out.println("Número\tCuadrado\tCubo");
	        for (int i = 0; i <= 10; i++) {
	            System.out.println(i + "\t" + i*i + "\t\t" + i*i*i);
	        }
	    }
	}

